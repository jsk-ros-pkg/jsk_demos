<launch>
  <!-- mongodb -->
  <include file="$(find mongodb_store)/launch/mongodb_store_inc.launch">
    <arg name="port" value="27017" />
    <arg name="use_daemon" value="false" />
    <arg name="machine" value="localhost" />
    <arg name="test_mode" value="true" />
  </include>

  <!-- map launch -->
  <include file="$(find jsk_maps)/launch/start_map_eng2.launch" />

  <!-- test node -->
  <test test-name="test_eng2_scene" pkg="roseus" type="roseus"
        args="$(find jsk_maps)/test/test-eng2-scene.l"
        time-limit="120" />

</launch>
