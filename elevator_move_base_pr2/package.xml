  <run_depend>jsk_topic_tools</run_depend>
  <run_depend>move_base_msgs</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>nodelet</run_depend>
  <run_depend>pr2_machine</run_depend>
  <run_depend>pr2eus</run_depend>
  <run_depend>roseus</run_depend>
  <run_depend>roseus_smach</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>rviz</run_depend>
  <run_depend>sound_play</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>std_srvs</run_depend>
  <run_depend>topic_tools</run_depend>

  <!-- Dependencies needed while testing this package. -->
  <test_depend>image_proc</test_depend>
  <test_depend>rosbag</test_depend>
  <test_depend>rostest</test_depend>

  <export>
    <app_manager app_dir="${prefix}/apps"/>
  </export>

</package>
