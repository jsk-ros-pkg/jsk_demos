<package>
  <name>elevator_move_base_pr2</name>
  <version>0.0.4</version>
  <description>elevator_move_base_pr2</description>
  <maintainer email="k-okada@jsk.t.u-tokyo.ac.jp">Kei Okada</maintainer>
