  <run_depend>app_manager</run_depend>
  <run_depend>cv_bridge</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>image_transport</run_depend>
  <run_depend>image_view</run_depend>
  <run_depend>imagesift</run_depend>
  <run_depend>jsk_data</run_depend>
  <run_depend>jsk_gui_msgs</run_depend>
  <run_depend>jsk_maps</run_depend>
  <run_depend>jsk_pcl_ros</run_depend>
  <run_depend>jsk_pcl_ros_utils</run_depend>
  <run_depend>jsk_perception</run_depend>
